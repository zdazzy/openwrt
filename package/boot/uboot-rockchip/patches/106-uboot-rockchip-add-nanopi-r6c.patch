@@ -196,6 +196,7 @@ dtb-$(CONFIG_ROCKCHIP_RK3588) += \
 	rk3588-orangepi-5-plus.dtb \
 	rk3588-quartzpro64.dtb \
 	rk3588s-rock-5a.dtb \
+	rk3588-nanopi-r6c.dtb \
 	rk3588-rock-5b.dtb
 
